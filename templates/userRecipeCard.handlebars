{{#each recipe as |recipe|}}
<div class="card col-md-4 col-sm-6 text-center">
  <div class="card-body">
    <img class="mx-auto col-12" src="{{recipe.image}}" alt="">
    <h5>{{recipe.label}}</h5>
    <button type="button" name="{{recipe.userDbId}}" class="button deleteRecipe">Remove Recipe</button>
  </div>
</div>


{{/each}}
